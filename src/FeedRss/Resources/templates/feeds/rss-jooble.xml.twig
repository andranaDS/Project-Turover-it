<job id="{{ jobPosting.id }}">
    <link>{{ jobPosting.link|raw }}</link>
    <name>{{ jobPosting.name|raw }}</name>
    <region>{{ jobPosting.region|raw }}</region>
    <salary>{{ jobPosting.salary|raw }}</salary>
    <description>{{ jobPosting.description|raw }}</description>
    <company>{{ jobPosting.company|raw }}</company>
    <pubdate>{{ jobPosting.pubDate|raw }}</pubdate>
    <updated>{{ jobPosting.updated|raw }}</updated>
    <jobtype>{{ jobPosting.jobType|raw }}</jobtype>
</job>
